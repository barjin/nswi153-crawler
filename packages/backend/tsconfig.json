{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "lib": ["ES6"],
    "target": "es6",
    "strict": false,
    "module": "CommonJS",
    "moduleResolution": "Node",
    "outDir": "./dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": true,
  },
  "include": ["src"]
}
