{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
        "../../.eslintrc.json",
        "eslint:recommended", 
        "plugin:react/recommended", 
        "plugin:react-hooks/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "plugins": ["react", "react-hooks"],
    "rules": {
        "react/react-in-jsx-scope": "off"
    }
}  